{
    "compileOnSave": false,
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "target": "es6",
        "outDir": "build",
        "moduleResolution": "node",
        "isolatedModules": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noLib": false,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./typings/main.d.ts",
        "./src/MMOG.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    },
    "files": [
        "./typings/main.d.ts",
        "./src/MMOG.ts"
    ]
}
